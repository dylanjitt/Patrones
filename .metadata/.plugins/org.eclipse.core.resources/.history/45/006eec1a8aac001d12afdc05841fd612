package practica4;

public class FactoryPasajes {
	
	public static Pasaje make(String type) {
		
		Pasaje pasaje;
		
		switch (type.toLowerCase()){
		case "regular": {
			
			pasaje = new PasajeRegular();
		}case "solidario": {
			
			pasaje = new PasajeSolidario();
		}case "infante": {
			
			pasaje = new PasajeInfantes();
		}
		
		default:
			pasaje = new PasajeRegular();
		}	
		return pasaje;
		
	}

}
