package practicaCambioMonedaSingleton;

public class CasaDeCambios {
	
private BancoCentral bci;

 public CasaDeCambios(BancoCentral bci) {
	this.bci = bci;
}
	
	public double dolaresBolivianos(double dolares) {
		double boliviano = bci.dolaresBolivianos(dolares);
		return boliviano;
	}
	
	public double bolivianosDolares(double boliviano) {
		double dolar = bci.bolivianosDolares(boliviano);
		return dolar;
	}
	
	public double eurosBolivianos(double euro) {
		double boliviano = bci.eurosBolivianos(euro);
		return boliviano;
	}
	
	public double bolivianosEuros(double boliviano) {
		double euro = bci.bolivianosEuros(boliviano);
		return euro;
	}

}
